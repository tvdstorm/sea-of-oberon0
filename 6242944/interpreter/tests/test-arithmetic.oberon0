MODULE test;
BEGIN
    AssertEqInt(0 * 0, 0);
    AssertEqInt(10 * 2, 20);
    AssertEqInt((10 * 1) * (3 * 2)*5, 300);

    AssertEqInt(4 DIV 2, 2);
    AssertEqInt(9 DIV 2, 4);
    AssertEqInt(0 DIV 222, 0);
    
    AssertEqInt(10 * 3 + 5, 35);
    AssertEqInt(5 + 10 * 3, 35);
    AssertEqInt((5 + 10) * 3, 45);
    AssertEqInt(3 * (5+10), 45);

    AssertEqInt((15 - 10) * (3 - 2), 5);
    AssertEqInt(15 - 10 * 3 + 57, 42);
    AssertEqInt(100 DIV 10 DIV 2, 5)
END test.