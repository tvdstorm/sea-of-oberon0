MODULE testmodule;

CONST
  MAXLENGTH	= 5;
  
VAR
  array: ARRAY MAXLENGTH OF INTEGER; 
  i,j,k: INTEGER;

BEGIN
 i := 0;
 j := 1;
 k := 25;
WHILE i<5 DO
 IF i < j THEN
	array[i] := j;
 ELSIF i = j THEN
	array[i] := k;
 ELSE
	array[i] := i;
 END;
 i := i + 1;
END
END testmodule.