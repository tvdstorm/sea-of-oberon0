MODULE module; 
	VAR i: INTEGER; 
	
PROCEDURE PrintHallo(VAR message: BOOLEAN); 
BEGIN 
	Write("hallo"); 
	WriteLn;
	Write(message);
	WriteLn; 
	message := 3; 
	Write(message);
	WriteLn
END PrintHallo; 

BEGIN 
	i := 1; 
	PrintHallo(i); 
	FunctieDieNietBestaat();
	Write(i) 
END module
