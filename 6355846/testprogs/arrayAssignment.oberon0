MODULE QuickSort;

CONST
  INPUTLENGTH = 5;

VAR
  input1: ARRAY INPUTLENGTH OF INTEGER;
  input2: ARRAY INPUTLENGTH OF INTEGER;
  input3: ARRAY INPUTLENGTH OF ARRAY 3 OF ARRAY INPUTLENGTH OF INTEGER;
  input4: ARRAY 5 OF INTEGER;

BEGIN
   input1 := input2;
   input4 := input3[0][0];
   input1 := input4
END QuickSort.