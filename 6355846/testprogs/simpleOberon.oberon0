  PROCEDURE Swap(VAR x, y: INTEGER);
  VAR
    temp: INTEGER;
  BEGIN
    temp := x;
    x := y;
    y := temp
  END Swap;

PROCEDURE Swap2;
  VAR
    temp: INTEGER;
  BEGIN
    temp := 5;
  END Swap2;

BEGIN
 i := 0;
 WHILE i < INPUTLENGTH DO
   i := i + 1
 END;
