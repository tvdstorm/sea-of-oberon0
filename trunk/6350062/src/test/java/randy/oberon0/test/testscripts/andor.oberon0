MODULE Andor;

VAR t, f: INTEGER;
BEGIN

t := (1 = 1);
f := (1 = 2);

IF t & t THEN
	Write(1)
ELSE
	Write(0)
END;

IF t & f THEN
	Write(1)
ELSE
	Write(0)
END;

IF f & t THEN
	Write(1)
ELSE
	Write(0)
END;

IF f & f THEN
	Write(1)
ELSE
	Write(0)
END;

IF t OR t THEN
	Write(1)
ELSE
	Write(0)
END;

IF t OR f THEN
	Write(1)
ELSE
	Write(0)
END;

IF f OR t THEN
	Write(1)
ELSE
	Write(0)
END;

IF f OR f THEN
	Write(1)
ELSE
	Write(0)
END;

IF t = t THEN
	Write(1)
ELSE
	Write(0)
END;

IF f = f THEN
	Write(1)
ELSE
	Write(0)
END;

IF f = t THEN
	Write(1)
ELSE
	Write(0)
END;

IF t = f THEN
	Write(1)
ELSE
	Write(0)
END;

IF t # t THEN
	Write(1)
ELSE
	Write(0)
END;

IF f # f THEN
	Write(1)
ELSE
	Write(0)
END;

IF f # t THEN
	Write(1)
ELSE
	Write(0)
END;

IF t # f THEN
	Write(1)
ELSE
	Write(0)
END;

IF f THEN
	Write(1)
ELSE
	Write(0)
END;

IF ~f THEN
	Write(1)
ELSE
	Write(0)
END;

END andor.

