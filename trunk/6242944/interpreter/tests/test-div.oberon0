MODULE test;
BEGIN
    AssertEqInt(1 DIV 1, 1);
    AssertEqInt(0 DIV 1, 0);
    AssertEqInt(10 DIV 2, 5);

    AssertEqInt(20 DIV (3-1), 10);
    AssertEqInt(20 DIV (3-5), 1-11);

    AssertEqInt(100 DIV 10 DIV 2, 5);
    AssertEqInt(1000 DIV 10 DIV 5 DIV 2, 10);
    AssertEqInt((1000 DIV 10) DIV (4 DIV 2), 50)
END test.