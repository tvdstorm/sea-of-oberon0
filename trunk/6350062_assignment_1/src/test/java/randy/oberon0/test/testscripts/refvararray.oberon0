MODULE refvar;

VAR getal: ARRAY 3 OF INTEGER;

PROCEDURE inc(VAR input: ARRAY 3 OF INTEGER);
  BEGIN
    input[1] := input[1] + 1
  END inc;

BEGIN
  Read(getal[0]);
  Read(getal[1]);
  Read(getal[2]);
  inc(getal);
  Write(getal[0]);
  Write(getal[1]);
  Write(getal[2])
END refvar.