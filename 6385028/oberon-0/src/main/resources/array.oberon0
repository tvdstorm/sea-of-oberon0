MODULE Array;
CONST
	INPUTLENGTH = 5;
VAR
	i, j : INTEGER;
	ar: ARRAY 5 OF INTEGER;
BEGIN
 i :=0;
 WHILE (i < INPUTLENGTH) DO
   ar[i] := i;
   Write(ar[i]);
   WriteLn;
   i := i + 1


 END;

   j := i;
   i := i + 1;
   WriteLn;
   WriteLn;
   Write(i);
   WriteLn;
   Write(j);
   WriteLn
END Array.